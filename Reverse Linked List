public class Solution 
{
    public static LinkedListNode<Integer> reverseLinkedList(LinkedListNode<Integer> head) 
    {
		LinkedListNode prev = null;
		LinkedListNode curr = head;
		LinkedListNode next;
		while(curr!=null){
			next = curr.next;
			curr.next=prev;

			prev=curr;
			curr=next;
		}
		return prev;
    }
}
